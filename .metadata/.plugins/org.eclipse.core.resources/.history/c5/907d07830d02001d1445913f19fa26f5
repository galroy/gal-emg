#ifndef INC_CLI_H_
#define INC_CLI_H_

typedef void (*HandlerFunc)(void*);

typedef struct comands_{
	char *comand;
	char *stat;
	HandlerFunc hendler;
	void *obj;
}COMANDS;

void RegisterCommand(char *comand,char *stat,HandlerFunc hendler,void *obj);

void cmdLed(void *obj,char *cmd);

void cliInit();

#endif /* INC_CLI_H_ */
