#include <stdint.h>
#include "MyMain.h"

#ifndef INC_CLOCK_H_
#define INC_CLOCK_H_

typedef enum timestate_{
	TIME_ON,
	TIME_OFF,
} TIME_STATE;

typedef struct _sysTime{
	TIME_STATE timeState;
	uint16_t year;
	uint16_t month;
	uint16_t day;
	uint16_t hour;
	uint16_t minute;
	uint16_t second;
	uint16_t milliSecond;
} SysTime;

#endif /* INC_CLOCK_H_ */
