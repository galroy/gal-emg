#include "main.h"
#include "cli.h"

COMANDS cmds[100];

void cliInit(){

	RegisterCommand("led","on","ledOn",&ledB);
	RegisterCommand("led","off","ledOff",&ledB);

}
void cmdLed(void *obj,char *cmd){
	if(strcmp(cmd, "on") == 0){
		ledOn(obj);
	}else if(strcmp(cmd, "off") == 0){
		ledOff(obj);
	}

}

void RegisterCommand(char *comand,char *stat,HandlerFunc hendler,void *obj){
	static int i = 0;
	cmds[i].comand = comand;
	cmds[i].stat = stat;
	cmds[i].obj = obj;
	cmds[i].hendler = Handler;
	i++;
}
