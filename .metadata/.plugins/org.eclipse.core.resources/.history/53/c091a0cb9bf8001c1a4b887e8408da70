#ifndef INC_DATETIME_H_
#define INC_DATETIME_H_

#include <stdint.h>
#include "main.h"

typedef struct _sysTime{

	uint16_t year;
	uint16_t month;
	uint16_t day;
	uint16_t hour;
	uint16_t minute;
	uint16_t second;
	uint32_t milliSecond;
} SYSTIME;

typedef struct _clock{
	//TIME_STATE timeState;
	SYSTIME sysTime;
	//TIM_HandleTypeDef *htim;
} CLOCK;

void clockInit(CLOCK *clk);

#endif /* INC_DATETIME_H_ */
